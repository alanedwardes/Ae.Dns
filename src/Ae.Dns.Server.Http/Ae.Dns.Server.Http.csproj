<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <OutputType>Exe</OutputType>
    <Version>0.0.4</Version>
    <WarningsAsErrors>true</WarningsAsErrors>

    <Authors>alanedwardes</Authors>
    <Description>DNS HTTP server with filtering for .NET Core</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/alanedwardes/Ae.Dns</PackageProjectUrl>
    <PackageTags>DNS;DNS Server;DNS UDP Server;DNS Filter</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Ae.Dns.Client\Ae.Dns.Client.csproj" />
    <ProjectReference Include="..\Ae.Dns.Server\Ae.Dns.Server.csproj" />
  </ItemGroup>

</Project>
